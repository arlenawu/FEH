SELECT * FROM lvl40_stats;
SELECT * FROM hero_skills;
SELECT * FROM super_assetflaws;
SELECT * FROM hero_availability;

-----------------------------------------------
SELECT * FROM lvl40_stats WHERE lvl40_stats.atk >= 40;

-- Get highest resistance units from The Binding Blade
SELECT * FROM lvl40_stats WHERE entry = 'The Binding Blade' ORDER BY res DESC;

-- Get all versions of character Azura
SELECT * FROM lvl40_stats WHERE hero_name = 'Azura'

-- Get highest speed units from Echoes: Shadows of Valentia
SELECT * FROM lvl40_stats WHERE entry = 'Echoes' ORDER BY spd DESC;